<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbung.musicapi.domain.genre.mapper.GenreMapper">

    <select id="findList" resultType="Genre">
        SELECT
        id, title, created_date, modified_date
        FROM genre
        ORDER BY title ASC
    </select>

</mapper>